function find(){
  cp = document.getElementById("cp").value;
  sp = document.getElementById("sp").value;
  if(sp>cp){
    profit = sp-cp;
    profit_percentage=(profit/cp)*100;
    document.getElementById("pl").innerHTML="profit ="+profit;
    document.getElementById("plp").innerHTML="profit % ="+profit_percentage;
  }
}



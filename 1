var index = students.length; // define leghth
    const student = {
      id: Date.now(),
      Title: document.getElementById('exampleInputEmail1').value,
      Dicription: document.getElementById('exampleFormControlTextarea1').value,
      gender: document.querySelector('input[id="bbb"]:checked').value,
      sel: document.querySelector('input[id="myChe"]:checked').value,
      vender: document.getElementById('ven').value,
      collection: document.getElementById('coll').value,
      tags: document.getElementById('tags').value,
    };
    students[index] = student;
    const jsonObj = JSON.stringify(students);
    console.log(jsonObj);
    localStorage.setItem("students", jsonObj);
  }
}


